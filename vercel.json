{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "server/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(check_session|signup|login|logout|trips|photos|users|trip-followers).*",
      "dest": "server/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/index.html"
    }
  ]
}