{
  "version": 2,
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/build",
  "builds": [
    {
      "src": "server/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(check_session|signup|login|logout|trips|photos|users|trip-followers).*",
      "dest": "server/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}